<?xml version="1.0" encoding="UTF-8"?>




<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">



	<display-name>Online_shopping_Store</display-name>
	<welcome-file-list>
		<welcome-file>Home.jsp</welcome-file>

	</welcome-file-list>


	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.servlet.Loginservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>in</servlet-name>
		<servlet-class>com.servlet.CustomerInsertservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>in</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Update</servlet-name>
		<servlet-class>com.servlet.UpdateCusservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>UpdateA</servlet-name>
		<servlet-class>com.servlet.UpdateAdminservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateA</servlet-name>
		<url-pattern>/updateA</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeleteB</servlet-name>
		<servlet-class>com.servlet.Deleteuserservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteB</servlet-name>
		<url-pattern>/deleteB</url-pattern>
	</servlet-mapping>





</web-app>



